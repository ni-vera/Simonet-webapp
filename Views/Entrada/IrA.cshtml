@model IEnumerable<webapp.Models.Pagina>
    
@{
    ViewData["Title"] = "Index";
}

@foreach (var item in Model)
{
        <a asp-action="IrA" asp-route-numpag="@(item.Id - 1)">Anterior</a>
        <form method="get" action="IrA">
            <input type="text" id="numpag" name="numpag">
            <input type="submit" value="Ir">
        </form>
        <a asp-action="IrA" asp-route-numpag="@(item.Id + 1)">Siguiente</a>
        <p>
        <a asp-action="Index">Volver a la lista de entradas</a>
        </p>
        <img src="@item.URL">
}


